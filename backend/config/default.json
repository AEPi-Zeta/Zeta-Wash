{
    "ZetaWash": {
        "Host": {
            "frontendURL": "http://localhost:4200",
            "backendURL": "http://localhost:8088/"
        },
        "Encryption": {
            "useEncryption": false,
            "certFilePath": "/etc/letsencrypt/live/example.com/fullchain.pem",
            "keyFilePath": "/etc/letsencrypt/live/example.com/privkey.pem"
        },
        "Machines": {
            "useQueue": true,
            "List": {
                "washer": {
                    "key": "washer",
                    "count": 2,
                    "default_minutes": 60,
                    "min_minutes": 1,
                    "max_minutes": 240,
                    "useOptions": true,
                    "useAlerts": true,
                    "minuteOptions": [
                        {
                            "key": "30",
                            "amount": 30,
                            "label": "30 - Quick"
                        },
                        {
                            "key": "60",
                            "amount": 60,
                            "label": "60 - Normal"
                        },
                        {
                            "key": "80",
                            "amount": 80,
                            "label": "80 - Long"
                        }
                    ],
                    "customAlerts": [
                        {
                            "key": "Stuff is Ready!",
                            "email": {
                                "subject": "[Zeta Wash - Notification] You've been buzzed! Your stuff is ready.",
                                "text": "Your stuff is ready!"
                            }
                        }
                    ],
                    "icon": "washer.png",
                    "email": {
                        "subject": "[Zeta Wash - Washer] Your clothes are washed!",
                        "text": "Your clothes finished washing at ${date}."
                    }
                },
                "Dryer": {
                    "key": "Dryer",
                    "email": {
                        "subject": "[Zeta Wash - Dryer] Your clothes are dry!",
                        "text": "Your clothes finished drying at ${date}."
                    },
                    "count": 2,
                    "default_minutes": 15,
                    "min_minutes": 0,
                    "max_minutes": 50,
                    "useOptions": false,
                    "useAlerts": false,
                    "icon": "dryer.png"
                }
            }
        },
        "Extra": {
            "titleTop": "Zeta Wash",
            "enableFullScreenButton": true
        },
        "Users": {
            "customUsersList": false,
            "forceCustomUsersList": false,
            "requirePinForSettings": true,
            "logAdminOnly": false,
            "removeMachineAdminOnly": false,
            "pinSet": true,
            "alertService": "None",
            "Email": {
                "service": "Gmail"
            }
        }
    }
}