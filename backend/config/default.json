{
    "ZetaWash": {
        "Host": {
            "frontendURL": "http://example.com",
            "backendURL": "http://example.com:8088/"
        },
        "Encryption": {
            "useEncryption": false,
            "certFilePath": "/etc/letsencrypt/live/example.com/fullchain.pem",
            "keyFilePath": "/etc/letsencrypt/live/example.com/privkey.pem"
        },
        "Machines": {
            "useQueue": false,
            "List": {
                "washer": {
                    "key": "washer",
                    "count": 2,
                    "default_minutes": 60,
                    "min_minutes": 0,
                    "max_minutes": 240,
                    "useOptions": true,
                    "minuteOptions": [
                        {
                            "key": "30",
                            "amount": 30,
                            "label": "30 - Quick"
                        },
                        {
                            "key": "60",
                            "amount": 60,
                            "label": "60 - Normal"
                        },
                        {
                            "key": "80",
                            "amount": 80,
                            "label": "80 - Long"
                        }
                    ],
                    "icon": "washer.png",
                    "email": {
                        "subject": "[Zeta Wash - Washer] Your clothes are washed!",
                        "text": "Your clothes finished washing at ${date}."
                    }
                },
                "dryer": {
                    "key": "dryer",
                    "count": 2,
                    "default_minutes": 50,
                    "min_minutes": 0,
                    "max_minutes": 240,
                    "useOptions": true,
                    "minuteOptions": [
                        {
                            "key": "30",
                            "amount": 30,
                            "label": "30 - Quick"
                        },
                        {
                            "key": "50",
                            "amount": 50,
                            "label": "50 - Normal"
                        },
                        {
                            "key": "80",
                            "amount": 80,
                            "label": "80 - Long"
                        }
                    ],
                    "icon": "dryer.png",
                    "email": {
                        "subject": "[Zeta Wash - Dryer] Your clothes are dry!",
                        "text": "Your clothes finished drying at ${date}."
                    }
                }
            }
        },
        "Extra": {
            "titleTop": "Zeta Wash",
            "enableFullScreenButton": true
        },
        "Users": {
            "customUsersList": false,
            "forceCustomUsersList": false,
            "requirePinForSettings": false,
            "pinSet": false,
            "alertService": "None",
            "Email": {
                "service": "Gmail"
            }
        }
    }
}