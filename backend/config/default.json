{
    "ZetaWash": {
        "Host": {
            "frontendURL": "http://localhost:4200",
            "backendURL": "http://localhost:8088/"
        },
      "Encryption": {
        "useEncryption": false,
        "certFilePath": "/etc/letsencrypt/live/aepizeta.ga/fullchain.pem",
        "keyFilePath": "/etc/letsencrypt/live/aepizeta.ga/privkey.pem"
      },
      "Machines": {
        "useQueue": true,
        "List": {
          "washer": {
            "key": "washer",
            "count": 2,
            "default_minutes": 34
          },
          "dryer": {
              "key": "dryer",
              "count": 2,
              "default_minutes": 40
          }
        }
      },
      "Extra": {
        "titleTop": "Zeta Wash"
      },
      "Users": {
        "customUsersList": true,
        "forceCustomusersList": false,
        "alertService": "email",
        "Email": {
          "service": "gmail",
          "auth": {
            "user": "",
            "pass": "" 
          },
          "dryerEmail": {
            "subject": "[Zeta Wash - Dryer] Your clothes are dry!",
            "text": "Your clothes finished drying at ${date}."
          },
          "washerEmail": {
            "subject": "[Zeta Wash - Dryer] Your clothes are washed!",
            "text": "Your clothes finished drying at ${date}."
          }
        }
      }
    }
}
